        "symfony/routing": "~2.3",
        "symfony/console": "~2.3",
        "symfony/stopwatch": "~2.3",
        "symfony/translation":"~2.3",
        "symfony/templating": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/monolog-bridge": "~2.3",
        "twig/twig": "~1.15",
        "twig/extensions": "~1.0",
        "joomla/joomla-cms":"~3.4",
