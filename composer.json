        "symfony/framework-bundle": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/process": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3,>=2.3.3",
